{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "arrow-parens": false,
        "member-ordering": {
            "options": {
                "order": [
                    "static-field",
                    "instance-field",
                    "public-static-method",
                    "constructor",
                    "instance-method",
                    "private-static-method"
                ]
            }
        },
        "object-literal-sort-keys": false,
        "no-console": true,
        "quotemark": false
    },
    "rulesDirectory": []
}
